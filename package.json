{
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "version": "1.1.2",
  "description": "instagram style stories in react js",
  "scripts": {
    "start:docs": "cd ./packages/documentation/ && yarn start",
    "build:stories": "cd ./packages/stories/ && yarn build:watch",
